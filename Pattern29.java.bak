class Pattern29 
{
	public static void main(String[] args) 
	{
			int spacecount=4;
			int starcount=1;
				for(int i=1;i<=9;i++)
		{
					for(int a=1;a<=spacecount;a++)
			{
						System.out.print(" ");
			}
			for(int b=1;b<=starcount;b++)
			{
				System.out.print("*");
			}
			if(i<5)
			{
				spacecount--;
				starcount++;
			}
			else
			{
				spacecount++;
				starcount--;
			}
		System.out.println();
		}
	}
}


				