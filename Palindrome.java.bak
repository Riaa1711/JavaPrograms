class Palindrome 
{
	public static void main(String[] args) 
	{
		int num=7658;
		int temp=num;
		int rev=0;
		while(num!=0)
		{
			int ld=num%10;
			rev=rev*10+ld;
			num=num/10;

	}
	if(rev==temp)
		{
		System.out.println("Palindrome number");
		}
		else
		{
			System.out.println(" Not a palindrome number");
		}
}
}
