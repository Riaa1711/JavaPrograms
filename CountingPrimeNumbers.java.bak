class CountingPrimeNumbers 
{
	public static void main(String[] args) 
	{
		int PrimeCount=0;
		for( int i=1;i<=100;i++)
		{
			int num=i;
			int count=0;
			for( int j=1;j<=num;j++){
				if(num%j==0)
			{
				count++;
			}
			}
			if (count==2)
			{
				PrimeCount++;
			}
			System.out.println(" the no is "+PrimeCount);
		}

	}
}
