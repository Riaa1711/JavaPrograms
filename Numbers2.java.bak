class Numbers2
{
	public static void main(String[] args) 
	{
		int num=145;
		int sum=0;
		int fact=1;
		int temp=num;
		while(num!=0)
		{
			int ld=num%10;
			for(int i=ld;i>=1;i--)
			{
				fact=fact*i;
			}
			sum=sum+fact;
			num=num/10;
		}
		if(sum==temp)
		{
			System.out.println("Strong number");
		}
		else
		{
			System.out.println("Not a strong number");
		}
		
	}
}
