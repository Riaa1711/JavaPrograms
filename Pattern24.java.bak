class Pattern24
{
	public static void main(String[] args) 
	{
		for(int i=1;i<=5;i++)
		{
			char ch='A';
		int num1=1;
			if(i%2==0)
			{
			for(int j=1;j<=5;j++)
			{
				if(i<=j)
				{
					System.out.print(num1);
					num1++;
				
				}
				else
				{
					System.out.print(" ");
				}
			}
			
				else 
			{
					for(int j=1;j<=5;j++)
				{
					if(i<=j)
					{
						System.out.print(ch);
						ch++;
					
					}else
					{
						System.out.print(" ");
					}
				}
			}
			
			else
				{
				System.out.print(" ");
				}
			}System.out.println();
		}
		
	}

	}



 
